  },
  "homepage": "https://github.com/nielsenjared/mern#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^3.6.1",
    "connect-mongo": "^2.0.3",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "leaflet": "^1.5.1",
    "mongoose": "^5.5.7",
    "mongoose-seed": "^0.6.0",
    "morgan": "^1.9.1",
    "npm": "^6.5.0",
    "passport": "^0.4.0",
